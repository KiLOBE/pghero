<div class="content">
  <h1>Foreign Tables</h1>

  <table class="table">
    <thead>
      <tr>
        <th>Schema</th>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @tables.each do |table| %>
        <tr>
          <td><%= table["schema"]%></td>
          <td><%= table["table"]%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

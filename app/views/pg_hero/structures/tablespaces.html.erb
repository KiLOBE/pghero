<div class="content">
  <h1>Tablespaces</h1>

  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @tablespaces.each do |tablespace| %>
        <tr>
          <td><%= tablespace["name"]%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

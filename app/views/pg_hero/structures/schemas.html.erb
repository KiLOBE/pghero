<div class="content">
  <h1>Schemas</h1>

  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @schemas.each do |schema| %>
        <tr>
          <td><%= schema["name"]%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
